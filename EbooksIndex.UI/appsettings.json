{
  "EbooksSourceDirectory": "",
  "OpenSearchUrl": "https://127.0.0.1:9200",
  "OpenSearchUser": "",
  "OpenSearchPass": "",
  "OpenSearchIndex": "",
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\SerilogEbookIndexer.txt",
          "outputTemplate": "{Timestamp:G} {Message} {NewLine:1} {Exception:1}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\SerilogEbookIndexer.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://localhost:5341"

        }
      }

    ]
  }
}
