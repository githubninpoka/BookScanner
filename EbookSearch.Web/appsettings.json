{
  "AllowedHosts": "*",
  "BooksOpenSearchOptions": {
    "OpenSearchUrl": "https://localhost:9200",
    "OpenSearchUser": "admin",
    "OpenSearchPass": "pass",
    "OpenSearchIndex": "ebooks"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\SerilogEbookSearchWeb.txt",
          "outputTemplate": "{Timestamp:G} {Message} {NewLine:1} {Exception:1}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\SerilogEbookSearchWeb.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://localhost:5341"

        }
      }

    ]
  }
}
